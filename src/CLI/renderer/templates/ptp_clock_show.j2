{% if value['network-transport'] == 'UDPv6' and value['unicast-multicast'] == 'multicast' %}
{{ 'IPv6 Scope'.ljust(21)}} {{ value['udp6-scope']}}
{% endif %}
{% endif %}
{% endif %}
{% if 'domain-number' in value.keys() %}
{{ 'Domain Number'.ljust(21)}} {{ value['domain-number']}}
{% endif %}
{% if 'clock-identity' in value.keys() %}
