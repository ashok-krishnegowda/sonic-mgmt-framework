{% endif %}
{% if 'priority1' in value.keys() %}
{{ 'Priority1'.ljust(21)}} {{ value['priority1']}}
{% endif %}
{% if 'priority2' in value.keys() %}
{{ 'Priority2'.ljust(21)}} {{ value['priority2']}}
{% endif %}
{% if 'two-step-flag' in value.keys() %}
{{ 'Two Step'.ljust(21)}} {% if value['two-step-flag'] == '0' %}
{%   print 'Disabled\n' %}
{% else %}
{%   print 'Enabled\n' %}
{% endif %}
{% endif %}
